{
  "name": "whs-pdf-analysis",
  "version": "1.0.0",
  "description": "A tool to extract data from a pdf file for Topix",
  "main": "src/server.ts",
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "format": "prettier --write ./src",
    "clean": "npx rimraf ./build",
    "build:css": "npx tailwindcss -i ./public/css/tailwind.css -o ./public/css/styles.css",
    "build": "npm run clean && npm run format && npm run lint && tsc && npm run build:css",
    "dev": "nodemon --watch ./src/** --ext * --exec ts-node ./src/server.ts",
    "start": "node ./build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pt232/whs-pdf-analysis.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pt232/whs-pdf-analysis/issues"
  },
  "homepage": "https://github.com/pt232/whs-pdf-analysis#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.3.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.13",
    "postcss": "^8.3.9",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.16",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
